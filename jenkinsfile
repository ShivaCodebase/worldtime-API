pipeline {
    agent any
  tools{
 maven "3.6.0"

}
 
    stages {
        stage ('Build') {
            steps {
                sh "mvn -version"
           
            }
        }

        stage ('final') {
            steps {
                echo 'This is a minimal pipeline.'
            }
        }
    }

post{
always{
cleanWs()
}
}

}
